{
  "name": "eslint-plugin-next-route-params",
  "version": "0.0.7",
  "description": "eslint rule to enforce correct route parameters for Next.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src && prettier src --check",
    "lint:fix": "eslint --fix src && prettier src --write",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "nextjs",
    "route",
    "params",
    "filebased",
    "approuter"
  ],
  "author": "Paul Heisner",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mando-C137/next-routeparams.git"
  },
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/utils": "^8.12.2",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/rule-tester": "^8.12.2",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-doc-generator": "^1.7.1",
    "eslint-plugin-eslint-plugin": "^6.3.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "type-fest": "^4.26.1"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": ">=8.5.0",
    "eslint": ">=8.57.0",
    "typescript": ">=5"
  },
  "files": [
    "dist"
  ]
}
